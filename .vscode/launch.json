  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Build",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/packages/gatsby",
      "program": "${workspaceFolder}/packages/gatsby/node_modules/.bin/gatsby",
      "args": ["build"],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
